{
    "title": "French AZERTY Swap @/# with </> keys",
    "rules": [
        {
            "description": "Swap @/# key with </> key on French AZERTY MacBook (including shift)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "non_us_backslash",
                        "modifiers": { "optional": ["any"] }
                    },
                    "to": [
                        {
                            "key_code": "grave_accent_and_tilde",
                            "modifiers": ["any"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "grave_accent_and_tilde",
                        "modifiers": { "optional": ["any"] }
                    },
                    "to": [
                        {
                            "key_code": "non_us_backslash",
                            "modifiers": ["any"]
                        }
                    ]
                }
            ]
        }
    ]
}
