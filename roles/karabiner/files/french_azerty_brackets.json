{
    "title": "French AZERTY Programming Brackets",
    "rules": [
        {
            "description": "Option (left/right) + top row keys for { } [ ]",
            "manipulators": [
                {
                    "type": "basic",
                    "from": { "key_code": "6", "modifiers": { "mandatory": ["left_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"{\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "6", "modifiers": { "mandatory": ["right_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"{\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "7", "modifiers": { "mandatory": ["left_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"}\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "7", "modifiers": { "mandatory": ["right_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"}\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "9", "modifiers": { "mandatory": ["left_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"[\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "9", "modifiers": { "mandatory": ["right_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"[\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "0", "modifiers": { "mandatory": ["left_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"]\"'" }]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "0", "modifiers": { "mandatory": ["right_option"] } },
                    "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to keystroke \"]\"'" }]
                }
            ]
        }
    ]
}
